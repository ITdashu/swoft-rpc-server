{
  "name": "swoft/rpc-server",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "rpc"
  ],
  "description": "microservice framework base on swoole",
  "license": "apache2.0",
  "require": {
    "swoft/framework": "dev-component",
    "swoft/console": "dev-master",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Rpc\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swoft\\Rpc\\Server\\Test\\": "test"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/swoft-cloud/swoft-console"
    },
    {
      "type": "git",
      "url": "https://github.com/swoft-cloud/framework"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ],
  "require-dev": {
    "eaglewu/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
